<% content_for :javascript do %>
  <script type="text/javascript">
    ShopifyApp.ready(function(){
      ShopifyApp.Bar.initialize({
        title: "Home"      });
    });
  </script>
<% end %>


<h2>List of Most Abandoned Checkouts</h2>

    <% @abandonedCheckouts.each do |abandonedCheckout| %>
    <input type="checkbox" name="@abandonedCheckout.customer.first_name"><%= link_to abandonedCheckout.id, "{@abandonedCheckout.abandoned_checkout_url}" %> </br>
  <% end %>
